{
    "name": "Blockxtension",
    "version": "1.0.1",
    "manifest_version": 3,
    "permissions": [
        "declarativeNetRequest"
    ],
    "declarative_net_request": {
        "rule_resources":[
            {
                "id": "ruleset_1", 
                "enabled": true, 
                "path": "rules.json"}
        ]
    },
    "description": "Blockxtension - My First Extension on Chrome, this extension block ads",
    "icons": {
        "128": "assets/img/logo-extension.png"
    },
    "action":{
    "default_popup": "popup.html"
    }
}